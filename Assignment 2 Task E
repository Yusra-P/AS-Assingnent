Module Module1

    Sub main()
        Dim a, d, f As String
        Dim b, c, e As Integer

	a=""
	d=""
	f=""
	b=0
	c=0
	e=0

        Console.Write("enter sentence: ")

        a = Console.ReadLine
        b = InStr(a, "not")
        c = InStr(a, "bad")
        d = Left(a, b - 1)
        c = c + 2
        e = Len(a)
        f = Right(a, (e - c))

        Console.WriteLine(d & "good" & f)

        Console.ReadKey()


    End Sub

End Module
